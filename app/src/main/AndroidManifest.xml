<?xml version="1.0"  encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.samiappscoll.kv.GKMaster">
    <uses-sdk
        android:minSdkVersion="16"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >

        <activity android:name="com.samiappscoll.kv.GKMaster.MainActivity"
            android:configChanges="keyboardHidden|orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name="com.samiappscoll.kv.GKMaster.DetailClass"
            android:screenOrientation="portrait"
            />
        <activity android:name="com.samiappscoll.kv.GKMaster.ResultActivity"/>
        <activity android:name="com.samiappscoll.kv.GKMaster.MissedQs"
            android:parentActivityName="com.samiappscoll.kv.GKMaster.ResultActivity"
            />

    </application>

</manifest>